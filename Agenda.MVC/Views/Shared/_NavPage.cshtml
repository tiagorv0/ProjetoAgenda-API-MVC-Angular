@model dynamic

<div class="row">
  <div class="col s5 offset-s5">
        <ul class="pagination">
          
          @for(int i = 1; i <= Model.TotalPages; i++)
          {
                @if(i == Model.CurrentPage)
                {
                    <li class="active teal lighten-2"><a href="@Url.Action("Index", new { page = i})">@i</a></li>
                }
                else
                {
                    <li class="waves-effect"><a href="@Url.Action("Index", new { page = i})">@i</a></li>
                }
          }
        </ul>
  </div>
</div>
